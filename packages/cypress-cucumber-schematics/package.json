{
  "name": "@ngchile/cypress-cucumber-schematics",
  "version": "1.0.0-rc.1",
  "description": "Schematics for create boilerplate and configurations for run cypress as e2e testing runner ",
  "author": "Gonzalo Pincheira <g.pincheira.a@gmail.com>",
  "license": "MIT",
  "builders": "./src/builders/builders.json",
  "schematics": "./src/schematics/collection.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngChile/ngchile-packages"
  },
  "scripts": {
    "test": "jest --coverage --watch",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage",
    "tsc": "tsc",
    "build": "npm run tsc",
    "prepublishOnly": "npm run build"
  },
  "jest-junit": {
    "outputDirectory": "./test-results/junit",
    "outputName": "results.xml"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.803.14",
    "@angular-devkit/core": "^8.3.20",
    "@angular-devkit/schematics": "^8.3.20",
    "cypress": "^3.5.0"
  },
  "peerDependencies": {
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@angular/cli": "^8.3.21",
    "@schematics/angular": "^8.3.20",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "cpr": "^3.0.1",
    "jest": "^24.7.1",
    "jest-junit": "^8.0.0",
    "ts-jest": "^24.2.0",
    "typescript": ">=3.4.0 <3.6.0"
  }
}
